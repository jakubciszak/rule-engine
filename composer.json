{
    "name": "jakubciszak/rule-engine",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.4",
        "phpstan/phpstan": "*"
    },
    "scripts": {
        "test:phpstan": [
            "APP_ENV=test php -d xdebug.mode=off vendor/bin/phpstan analyse -c phpstan.neon --no-progress --no-interaction --no-ansi"
        ]
    },
    "autoload": {
        "psr-4": {
            "JakubCiszak\\RuleEngine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JakubCiszak\\RuleEngine\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Jakub Ciszak",
            "email": "j.ciszak@gmail.com"
        }
    ]
}
